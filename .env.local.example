# OnchainKit API Key (get from https://portal.cdp.coinbase.com/)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_api_key_here

# Optional: Custom RPC endpoints
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org

# Database
DATABASE_URL="postgresql://username:password@localhost:5432/orbit_db"

# OpenAI API Key (for AI insights)
OPENAI_API_KEY=your_openai_api_key_here

# Plaid API (for financial data)
PLAID_CLIENT_ID=your_plaid_client_id
PLAID_SECRET=your_plaid_secret
PLAID_ENV=sandbox

# Google Calendar API
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Farcaster API (if needed)
FARCASTER_API_KEY=your_farcaster_api_key

# Upstash Redis (for caching and sessions)
UPSTASH_REDIS_REST_URL=your_upstash_redis_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_token

# MiniKit Configuration
NEXT_PUBLIC_MINIKIT_APP_ID=your_minikit_app_id

# Health Data APIs (optional)
APPLE_HEALTHKIT_KEY_ID=your_apple_key_id
APPLE_HEALTHKIT_PRIVATE_KEY=your_apple_private_key
GOOGLE_FIT_CLIENT_ID=your_google_fit_client_id
GOOGLE_FIT_CLIENT_SECRET=your_google_fit_client_secret

# Notification service (optional)
NOTIFICATION_WEBHOOK_URL=your_notification_webhook_url

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_ENVIRONMENT=development

